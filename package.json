{
	"name": "kidari-teacher",
	"version": "0.1.0",
	"private": true,
	"engines": {
		"node": ">=22 <23",
		"pnpm": ">=10.11.0 <11.0.0"
	},
	"scripts": {
		"dev": "next dev --turbopack",
		"dev:test": "NODE_ENV=test npm run dev",
		"prebuild": "prisma generate",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"format": "prettier --write .",
		"lint-staged": "lint-staged",
		"prepare": "node -e \"process.env.CI && process.exit(0)\" || husky",
		"cypress:open": "dotenv -e .env.test -- npx prisma db push --force-reset --skip-generate && cypress open",
		"cypress:run": "dotenv -e .env.test -- npx prisma db push --force-reset --skip-generate && cypress run",
		"test:e2e": "dotenv -e .env.test -- npx prisma db push --force-reset --skip-generate && start-server-and-test dev http://localhost:3000 cypress:run"
	},
	"dependencies": {
		"@hookform/resolvers": "^5.2.1",
		"@next-auth/prisma-adapter": "^1.0.7",
		"@prisma/client": "^6.12.0",
		"@radix-ui/react-alert-dialog": "^1.1.14",
		"@radix-ui/react-dialog": "^1.1.14",
		"@radix-ui/react-dropdown-menu": "^2.1.15",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-popover": "^1.1.14",
		"@radix-ui/react-progress": "^1.1.7",
		"@radix-ui/react-scroll-area": "^1.2.9",
		"@radix-ui/react-select": "^2.2.5",
		"@radix-ui/react-separator": "^1.1.7",
		"@radix-ui/react-slot": "^1.2.3",
		"@radix-ui/react-tabs": "^1.1.12",
		"@supabase/ssr": "^0.6.1",
		"@supabase/supabase-js": "^2.55.0",
		"@tanstack/react-query": "^5.84.2",
		"@trpc/client": "^11.3.1",
		"@trpc/next": "^11.3.1",
		"@trpc/react-query": "^11.3.1",
		"@trpc/server": "^11.3.1",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"date-fns": "^4.1.0",
		"framer-motion": "^12.23.12",
		"lucide-react": "^0.539.0",
		"next": "^15.4.6",
		"next-auth": "^4.24.11",
		"next-themes": "^0.4.6",
		"react": "^19.1.1",
		"react-day-picker": "^9.8.1",
		"react-dom": "^19.1.1",
		"react-hook-form": "^7.62.0",
		"sonner": "^2.0.5",
		"superjson": "^2.2.2",
		"tailwind-merge": "^3.3.1",
		"tailwindcss-animate": "^1.0.7",
		"zod": "^4.0.17"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@tailwindcss/postcss": "^4.1.8",
		"@testing-library/cypress": "^10.0.3",
		"@types/node": "^22.17.1",
		"@types/react": "^19.1.9",
		"@types/react-dom": "^19.1.7",
		"axe-core": "^4.10.3",
		"cypress": "^14.5.1",
		"cypress-axe": "^1.6.0",
		"dotenv-cli": "^8.0.0",
		"eslint": "^9.33.0",
		"eslint-config-next": "^15.4.6",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.5",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prisma": "^6.12.0",
		"start-server-and-test": "^2.0.12",
		"tailwindcss": "^4.1.8",
		"typescript": "^5.9.2"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"prettier --write"
		]
	}
}
